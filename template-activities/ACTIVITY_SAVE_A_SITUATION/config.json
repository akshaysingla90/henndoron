    "moduleStart": {
      "ops": [
        {
          "attributes": {
            "italic": true,
            "bold": true
          },
          "insert": "Oh no! Something’s gone wrong! "
        },
        {
          "attributes": {
            "italic": true
          },
          "insert": "The dog, the cat, and the sheep are in the tub! But there are "
        },
        {
          "attributes": {
            "italic": true,
            "bold": true
          },
          "insert": "holes"
        },
        {
          "attributes": {
            "italic": true
          },
          "insert": " in the tub! Let’s "
        },
        {
          "attributes": {
            "italic": true,
            "bold": true
          },
          "insert": "plug the tub!"
        },
        {
          "attributes": {
            "italic": true
          },
          "insert": " What can we use to plug the tub?"
        },
        {
          "insert": " \n\n"
        },
        {
          "attributes": {
            "underline": true
          },
          "insert": "Enable student mouse."
        },
        {
          "insert": " \n\n"
        },
        {
          "attributes": {
            "italic": true
          },
          "insert": "We can plug the tub with a"
        },
        {
          "attributes": {
            "italic": true,
            "bold": true
          },
          "insert": " tree/book/house/hat/stickers"
        },
        {
          "attributes": {
            "italic": true
          },
          "insert": "! Do you want a ___? YES - I want a ____."
        },
        {
          "insert": "\n\n"
        },
        {
          "attributes": {
            "underline": true
          },
          "insert": "Click on the item in your sidebar to ‘give’ it to the student. This will make it draggable in the students sidebar."
        },
        {
          "insert": "\n\n"
        },
        {
          "attributes": {
            "italic": true
          },
          "insert": "Click and drag to plug the hole in the tub!"
        },
        {
          "insert": "\n\n"
        },
        {
          "attributes": {
            "underline": true
          },
          "insert": "Repeat for all holes in the tub."
        },
        {
          "insert": "\n\n"
        },
        {
          "attributes": {
            "italic": true
          },
          "insert": "Wonderful! You plugged the tub with a "
        },
        {
          "attributes": {
            "italic": true,
            "bold": true
          },
          "insert": "tree/book/house/hat/stickers"
        },
        {
          "attributes": {
            "italic": true
          },
          "insert": "! "
        },
        {
          "insert": "\n"
        }
      ]
    }
  },
